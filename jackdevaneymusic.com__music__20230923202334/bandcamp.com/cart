            <!-- /ko -->
           
            <h4 class="item-title">
                <a data-bind="attr: { href: tralbumURL }" target="_blank">
                    <span data-bind="text: title"></span> <span class="item-artist">by <span data-bind="text: artist"></span></span>
                </a>
            </h4>

            <p class="item-genre" data-bind="text: genre"></p>
            <p class="item-desc"><span data-bind="html: desc"></span></p>

            <!-- ko if: videoAttributes -->
            <ul class="item-collection-controls horizontal-list">
                <li class="buy-now">
