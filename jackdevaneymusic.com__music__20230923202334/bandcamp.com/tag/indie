    <!-- use that same template for the knockout render -->
    <script type="text/html" id="dig-deeper-item"><div class="col col-3-15 dig-deeper-item item" data-bind="css: { 'playing': playing }">
    <div class="art ratio-1-1" data-bind="click: $parent.playItem.bind(null, $data)">
        <img data-bind="attr: { 'src': artURL }" src="https://f4.bcbits.com/img/blank.gif">
        <div class="art-play" data-bind="visible: !empty">
            <div class="art-play-bg"></div>
            <div class="art-play-icon"></div>
        </div>
    </div>
    <div class="info">
        <a target="_blank" data-bind="attr: {'href': tralbumUrl}, click: $parent.stat.bind(null, 'tralbum_click', $data)">
            <div class="title" data-bind="text: title"></div>
            <div class="artist" data-bind="visible: !empty">
                
                by <span data-bind="text: artist"></span>
                
            </div>
