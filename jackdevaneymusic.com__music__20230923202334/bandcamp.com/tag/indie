    <!-- knockout rendered - shown on bind -->
    <!-- ko if: true -->
    <div class="" style="display: none;" data-bind="
        visible: true,
        template: { 'name': 'dig-deeper-item', foreach: items }
    ">
    </div>

    <div style="display: none;" class="result-empty" data-bind="
        with: filterSettings,
        visible: !items() || items().length === 0
    ">
        <div class="content">
            <img src="/img/discover/tumbleweed-emptystate-lightbg.gif">
            <span class="text" data-bind="
                click: resetFilters,
                clickBubble: false
            ">Start over.</span>
        </div>
    </div>
