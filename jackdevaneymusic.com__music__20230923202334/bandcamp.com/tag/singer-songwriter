    <!-- ko if: true -->
    <div class="row carousel" style="display: none;" data-bind="
        visible: true,
        foreach: pages,
        css: { 'rendered': rendered, 'paging-next': pagingNext, 'paging-prev': pagingPrev, 'no-anim': preventAnimation }
    ">
        <div class="page" data-bind="
            template: { 'name': 'new-releases-carousel-item', foreach: items },
            css: position
        ">
        </div>
    </div>
    <!-- /ko -->

    <div class="lower-button next-button" data-bind="event: { 'mousedown': next }, visible: nextAvailable" >
        <svg class="dark" width="30px" height="24px" viewBox="0 0 28 24"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#hub-page-next"></use></svg>
