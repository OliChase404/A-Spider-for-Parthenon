                                    <!-- /ko -->

                                    <!-- ko if: !$parent.selected() || !$parent.selected().isCustom() -->
                                        <input type="text" class="custom-location-search" data-bind="
                                            visible: $parent.searchVisible,
                                            textInput: $parent.searchTerm,
                                            hasFocus: $parent.searchHasFocus,
                                            event: { 'keydown': $parent.exactMatchKeypress }
                                        ">

