                                <!-- /ko -->

                                <!-- ko if: selected().length > 0 -->
                                    <span class="add-another">add another tag</span>
                                <!-- /ko -->
                            </button>

                            <div data-bind="visible: expanded">
                                <input type="text" data-bind="
                                    textInput: searchTerm,
                                    hasFocus: searchHasFocus,
                                    event: { 'keydown': searchFieldKeypress }
                                ">
                                <button class="select-tag" data-bind="click: selectTag, disable: selectTagDisabled">
                                    <svg width="16" height="16" viewBox="0 0 24 24"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#material-arrow-fwd"></use></svg>
                                </button>
                            </div>

                            <ol class="filter-select search-results" data-bind="foreach: searchResults, css: { 'expanded': searchResults().length > 0 }">
                                <li tabindex="0" data-bind="
                                    click: function () { $parent.selectSearchResult($data); },
                                    event: { 'keydown': $parent.searchResultKeypress },
                                ">
                                    <span data-bind="html: displayName"></span>
                                </li>
                            </ol>
                        </div>
                    <!-- /ko -->

                    <!-- ko if: name() !== "tags" -->
                        <!-- ko if: name() === "format" -->
                        <svg class="filter-icon format" viewBox="0 0 27 20">
                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#material-format"></use>
                        </svg>
                        <!-- /ko -->

                        <!-- ko if: name() === "location" -->
                        <svg class="filter-icon location" viewBox="0 0 17 20">
                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#material-location"></use>
                        </svg>
                        <!-- /ko -->

