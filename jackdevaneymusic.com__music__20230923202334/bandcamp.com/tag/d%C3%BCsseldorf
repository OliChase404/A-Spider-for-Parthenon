                        </svg>
                        <!-- /ko -->

                        <button class="show-filter-select" data-bind="click: toggleExpanded, clickBubble: false, attr: { 'id': 'show-filter-select-' + name() }">

                            <span data-bind="html: selectedStr"></span>

                            <svg class="expand-icon expand" viewBox="0 0 24 24"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#hubs-down"></use></svg>
                            <svg class="expand-icon collapse" viewBox="0 0 24 24"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#hubs-up"></use></svg>
                        </button>

                        <!-- ko if: name() === "location" -->
                        <ol class="filter-select" data-bind="foreach: displayOptions, css: { 'expanded': expanded }">
                            <!-- ko if: $index() === 1 -->
                                <li tabindex="0" class="custom-location-item" data-bind="
                                    click: $parent.searchClick,
                                    clickBubble: false,
                                    css: { 'fake-focus': $parent.searchHasFocus },
                                    event: { 'keydown': $parent.searchKeypress },
                                ">
                                    <!-- ko if: $parent.selected() && $parent.selected().isCustom() -->
                                        <span data-bind="text: $parent.selected().fullName"></span>
                                        <svg class="close-icon" viewBox="0 0 24 24"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#material-close"></use></svg>
                                    <!-- /ko -->

                                    <!-- ko if: !$parent.selected() || !$parent.selected().isCustom() -->
                                        <input type="text" class="custom-location-search" data-bind="
                                            visible: $parent.searchVisible,
                                            textInput: $parent.searchTerm,
                                            hasFocus: $parent.searchHasFocus,
                                            event: { 'keydown': $parent.exactMatchKeypress }
                                        ">

                                        <span data-bind="visible: !$parent.searchVisible()">
                                            set custom location
                                        </span>
                                        <button class="select-exact-loc" data-bind="
                                            attr:{ 'disabled': !$parent.hasExactMatch()},
                                            css: { 'exact-match': $parent.hasExactMatch()},
                                            click: $parent.selectExactMatch,
                                            event: { 'keydown': $parent.exactMatchKeypress }
                                        ">
                                            <svg width="16" height="16" viewBox="0 0 24 24"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#material-arrow-fwd"></use></svg>
                                        </button>
                                    <!-- /ko -->
                                </li>
                            <!-- /ko -->

                            <li tabindex="0" data-bind="
                                html: displayName,
                                click: $parent.select,
                                clickBubble: false,
                                css: { 'selected': selected },
                                event: { 'keydown': $parent.locationKeypress },
                            "></li>
                        </ol>
                        <!-- /ko -->

                        <!-- ko if: name() !== "location" -->
                        <ol class="filter-select" data-bind="foreach: options, css: { 'expanded': expanded }">
                            <li tabindex="0" data-bind="
                                text: name,
                                click: $parent.select,
                                event: { 'keydown': $parent.keypress },
                                css: { 'selected': selected }
                            "></li>
                        </ol>
                        <!-- /ko -->
                    <!-- /ko -->
                </li>
                <!-- /ko -->
                <li class='reset'>
                    <button class="show-filter-select" data-bind="
                        click: resetFilters,
                        clickBubble: false,
                        visible: !areDefault()
                    ">
                        <span>reset filters</span>
                    </button>
                </li>
            </ol>

