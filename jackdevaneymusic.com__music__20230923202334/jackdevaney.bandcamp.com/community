    data-test="comment-input"
    data-bind="with: commentInputVM"
>
    <a class="bio-photo-container" data-bind="
        css: { 'blank': !bioImageId(), 'editable': canEditBioImage },
        attr: {
            'tabindex': canEditBioImage() ? 0 : -1,
            'href': canEditBioImage() ? '#' : null
        },
        click: onBioPhotoClick
    ">
        <!-- ko if: bioImageId -->
            <img
                class="fan-photo"
                data-bind="src_image: {
                    'image_id': bioImageId(),
                    'format': 'fan_bio_thumb_small'
            }" />
        <!-- /ko -->

        <!-- ko if: !bioImageId() && canEditBioImage() -->
            <svg class="camera-icon" viewBox="0 0 16 16"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#ic-camera-small"></use></svg>
        <!-- /ko -->
    </a>

    <textarea
        data-bind="
            attr: {
                'maxLength': maxLength,
                'placeholder': placeholder,
                'rows': rows
            },
            textInput: comment,
            hasFocus: commentInputHasFocus,
            selectionRange: commentSelectionRange,
            event: {
                keypress: onKeypress,
                keyup: maybeInsertEmoji,
                blur: onBlur
            },
            enable: !inputDisabled()
        "
        type="text"
    ></textarea>

    
</div>


        </form>
    </div>
<!-- /ko -->

    </li>
</script>

